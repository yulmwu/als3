{
    "name": "als3",
    "version": "0.0.1",
    "description": "A Lightweight STaaS Storage Service",
    "author": "yulmwu",
    "private": true,
    "license": "MIT",
    "workspaces": [
        "frontend",
        "backend",
        "shared"
    ],
    "scripts": {
        "shared:build": "npm run build --workspace=shared",
        "backend:dev": "npm run dev --workspace=backend",
        "backend:build": "npm run build --workspace=backend",
        "backend:start": "npm run start --workspace=backend",
        "frontend:dev": "npm run dev --workspace=frontend",
        "frontend:build": "npm run build --workspace=frontend",
        "frontend:start": "npm run start --workspace=frontend",
        "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
        "prettier:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\""
    },
    "devDependencies": {
        "prettier": "^3.6.2"
    },
    "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
